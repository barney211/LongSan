/**app.wxss**/
page{
  width:100%;
  height:100%;
  /*height:100vw;*/
  box-sizing: border-box;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.commonBottom{
  width:100%;
  height:60px;
  line-height: 15px;
  background: #04A192;
  color:#fff;
  font-size: 14px;
  position: absolute;
  bottom: 0;
  left:0;
}
.commonBottom>view{
  margin-left: 10px;
  margin-top: 3.5px;
}
